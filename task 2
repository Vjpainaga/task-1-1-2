class task  {

    public static void main (String[] args){

        int result = 1 + 2; // result is now 3
        System.out.println(result);

        result = 3 - 1; // result is now 2
        System.out.println(result);

        result = 2 * 2; // result is now 4
        System.out.println(result);

        result = 4 / 2; // result is now 2
        System.out.println(result);

        result = 2 + 8; // result is now 10
        result = 4 - 7; // result is now 3
        System.out.println(result);

    }
}
